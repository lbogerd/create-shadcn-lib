{
	"name": "create-shadcn-lib",
	"version": "0.0.1",
	"description": "the fastest way to build your own component library",
	"packageManager": "pnpm@10.10.0",
	"type": "module",
	"engines": {
		"node": ">=23.0.0"
	},
	"main": "index.js",
	"scripts": {
		"dev": "concurrently \"pnpm run watch\" \"pnpm run start\"",
		"watch": "tsdown --watch",
		"build": "tsdown",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"@clack/core": "0.4.2",
		"@clack/prompts": "0.10.1",
		"chalk": "5.4.1",
		"commander": "13.1.0"
	},
	"devDependencies": {
		"@types/node": "^22",
		"concurrently": "^9",
		"prettier": "^3",
		"typescript": "^5",
		"tsdown": "0.11.1"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 2,
		"semi": false
	},
	"keywords": [],
